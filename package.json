{
  "name": "lb4-react-ssr-redux",
  "version": "1.0.0",
  "description": "A basic loopback application with react and redux.",
  "keywords": [
    "loopback-application",
    "loopback",
    "react",
    "redux"
  ],
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc es2017 --outDir dist --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha dist/test",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "nodemon index.js",
    "build:client": "webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^1.0.8",
    "@loopback/context": "^1.4.0",
    "@loopback/core": "^1.1.3",
    "@loopback/rest": "^1.5.1",
    "@loopback/rest-explorer": "^1.1.4",
    "@loopback/service-proxy": "^1.0.5",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "p-event": "^2.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@loopback/build": "^1.1.0",
    "@loopback/testlab": "^1.0.3",
    "@loopback/tslint-config": "^1.0.0",
    "@types/express": "^4.11.1",
    "@types/node": "^10.12.18",
    "@types/p-event": "^1.3.0",
    "@types/got": "^9.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.16.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "husky": "^0.14.3",
    "nodemon": "^1.13.3",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.6.2",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.0.0"
  }
}
